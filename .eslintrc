{
    "extends": [
        "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true,
        "shared-node-browser": true
    },
    "rules": {
        "no-console": 0,
        "no-irregular-whitespace": 0,
        "eqeqeq": 2,
        "no-unused-vars": 1
    }
}
